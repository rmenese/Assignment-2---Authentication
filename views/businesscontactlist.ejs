<%- include partials/header.ejs %>

<%- include partials/main_nav.ejs %> 

  <!-- Main Content -->
  <main class="container">
    <div class="row">
        <div class="offset-md-3 col-md-6">
            <h1>Business Contacts</h1>
            
            <br>
  
            <table class="table table-bordered table-striped table-hover">
                <!-- Header Row-->
                  <tr>
                  
                      <th class="text-center">Contact Name</th>
                      <th class="text-center">Contact Number</th>
                      <th class="text-center">Email</th>
                      
                  </tr>
  
                  <!--Repeatable Temple Row -->
  
                  <% for (let count = 0; count < businesscontactsList.length; count++) { %>
                        
                  
                      <tr>
                          
                                  <td class="text-center"><%= businesscontactsList[count].contactname %></td>
                                  <td class="text-center"><%= businesscontactsList[count].contactnumber %></td>
                                  <td class="text-center"><%= businesscontactsList[count].email %></td>
                                  
                          
                      </tr>
                  
                    <% } %>
                  
              </table>
        </div>
    </div>
  
<%- include partials/bottom_nav.ejs %>

<%- include partials/footer.ejs %>














