<%- include ../partials/header.ejs %>

<%- include ../partials/main_nav.ejs %> 

<!-- Content for the Edit Page -->
<div class="container">
    <div class="row">
        <div class="offset-md-2 col-md-8">
        <h1><%= title %></h1>

        <form  class="form" method = "POST">
            <div class="form-group">
                <label for="contactnameTextField"> Contact Name</label>
                <input type="text" class="form-control"
                id="contactnameTextField"
                placeholder="Enter the Contact name"
                name="contactname" 
                value="<%= businesscontactlist.contactname %>"
                required>
            </div>

            <div class="form-group">
                <label for="contactnumberTextField"> Contact Number</label>
                <input type="text" class="form-control"
                id="contactnumberTextField"
                placeholder="Enter the Contact number"
                name="contactnumber" 
                value="<%= businesscontactlist.contactnumber %>"
                required>
            </div>

            <div class="form-group">
                <label for="emailTextField"> Email</label>
                <input type="text" class="form-control"
                id="emailTextField"
                placeholder="Enter the Email"
                name="email" 
                value="<%= businesscontactlist.email %>"
                required>
            </div>

            <button class="btn btn-primary" type="submit">
                <i class="fas fa-edit"></i> Submit</button>

                <a href="/businesscontactlist-list" class="btn btn-warning">
                    <i class="fas fa-undo"></i>< Cancel/a>

        </form>
    </div>
    
    </div>
</div>





<%- include ../partials/bottom_nav.ejs %>

<%- include ../partials/footer.ejs %>